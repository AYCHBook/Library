<% if @contributors.length > 0 %>
	<div class="row">
	  <div class="col-md-12">
	  	<hr>
	    <h3 data-ga-tracked-el='top-contributors'>Contributors</h4>
	    <%= render @contributors %>
	  </div>
	  <div class='col-md-12'>
	  	<br><%= link_to 'See all contributors', repository_contributors_path(@repository.to_param) %>
	  </div>
	</div>
<% end %>