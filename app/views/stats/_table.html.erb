<div class="col-md-4">
  <h4><%= title %></h4>
  <table class='table table-striped table-hover'>
    <% records.each do |date,count| %>
      <tr>
        <td>
          <%= date.to_s(:long) %>
        </td>
        <td>
          <%= number_with_delimiter count %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
